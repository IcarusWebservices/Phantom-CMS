{
    "logicPackName": "Twenties Default Logic Pack",
    "routes": {
        "/": {
            "controller": "Twenties_Index",
            "method": "index",
            "importance": 10
        },
        "/posts": {
            "controller": "demopostscontroller",
            "method": "overview",
            "importance": 1000
        },
        "/posts/:post_slug": {
            "controller": "demopostscontroller",
            "method": "single",
            "importance": 1000
        }
    },
    "editors": {
        "record_types": {
            "demopost": {
                "primary": [
                    {
                        "fieldslug": "demotextfield",
                        "exportid": "content",
                        "required": true
                    }
                ]
            }
        }
    }
}